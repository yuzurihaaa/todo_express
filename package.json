{
  "name": "todo-app",
  "version": "0.0.0",
  "private": true,
  "main": "build/start.js",
  "scripts": {
    "test": "none",
    "start-dev": "nodemon --config \"./util/nodemon.json\"/",
    "build": "tsc",
    "start": "ts-node src/start.ts"
  },
  "dependencies": {
    "@overnightjs/core": "^1.6.9",
    "@overnightjs/logger": "^1.1.9",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "^4.17.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.17.0",
    "@types/http-errors": "^1.6.1",
    "@types/morgan": "^1.7.36",
    "@types/node": "^12.6.8",
    "eslint-config-xo": "^0.26.0",
    "eslint-config-xo-lass": "^1.0.3",
    "eslint-config-xo-typescript": "^0.15.0",
    "husky": "^3.0.1",
    "nodemon": "^1.19.1",
    "prettier": "1.18.2",
    "ts-node": "^8.3.0",
    "ts-node-dev": "^1.0.0-pre.40",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3",
    "xo": "^0.24.0"
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "xo --fix",
        "prettier --write",
        "git add"
      ],
      "*.ts": [
        "xo --fix",
        "prettier --write",
        "git add"
      ],
      "package.json": [
        "fixpack",
        "git add"
      ]
    },
    "xo": {
      "prettier": true,
      "space": true,
      "semi": false,
      "semicolon": false,
      "extends": [
        "xo-lass",
        "xo-typescript"
      ]
    }
  }
}
